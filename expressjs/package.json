{
	"name": "expressjs",
	"scripts": {
		"dev": "yarn ts-prepare && nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
		"ts-prepare": "ts-patch install -s",
		"build": "tsc && tsc-alias",
		"start": "node dist/index.js",
		"postinstall": "tsc && tsc-alias"
	},
	"dependencies": {
		"cors": "2.8.5",
		"dotenv": "16.4.1",
		"express": "4.18.2",
		"morgan": "1.10.0"
	},
	"devDependencies": {
		"@types/cors": "2.8.17",
		"@types/express": "4.17.21",
		"@types/morgan": "1.9.9",
		"@types/node": "20.11.14",
		"nodemon": "3.0.3",
		"ts-node": "10.9.2",
		"ts-patch": "3.2.1",
		"tsc-alias": "1.8.10",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.3.3",
		"typescript-transform-paths": "3.4.7"
	}
}
